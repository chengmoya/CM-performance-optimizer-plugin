{
  "manifest_version": 1,
  "name": "CM性能优化插件",
  "version": "3.0.0",
  "description": "通过缓存数据库查询结果来优化MaiBot性能。包含消息缓存和人物信息缓存两个模块，可减少95%以上的数据库查询，显著降低响应延迟",
  "author": {
    "name": "城陌",
    "url": "https://github.com/chengmoya"
  },
  "license": "GPL-3.0",
  "host_application": {
    "min_version": "0.11.0",
    "max_version": "1.0.0"
  },
  "repository_url": "https://github.com/chengmoya/CM-performance-optimizer-plugin",
  "keywords": [
    "性能优化",
    "缓存",
    "数据库",
    "消息缓存",
    "人物缓存",
    "performance",
    "cache",
    "optimization"
  ],
  "categories": [
    "System",
    "Performance"
  ],
  "tags": [
    "性能",
    "优化",
    "缓存"
  ],
  "dependencies": [],
  "python_dependencies": [],
  "config_file": "config.toml",
  "enabled": true,
  "features": {
    "message_cache": {
      "enabled": true,
      "description": "消息缓存 - 缓存find_messages查询结果，命中率>95%"
    },
    "person_cache": {
      "enabled": true,
      "description": "人物信息缓存 - 缓存用户昵称等信息，命中率>90%"
    }
  },
  "configuration": {
    "modules": {
      "message_cache_enabled": true,
      "person_cache_enabled": true,
      "expression_cache_enabled": false,
      "slang_cache_enabled": false
    }
  },
  "id": "chengmoya.CM-performance-optimizer"
}
