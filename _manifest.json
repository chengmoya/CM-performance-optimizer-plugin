{
  "manifest_version": 1,
  "name": "CM-performance-optimizer",
  "version": "6.0.0",
  "description": "性能优化 - 消息缓存 + 人物信息缓存 + 表达式缓存 + 黑话缓存 + 知识库图谱缓存 + 通知系统 + 错误日志通知",
  "author": {
    "name": "城陌"
  },
  "license": "MIT",
  "main": "plugin.py",
  "dependencies": [],
  "python_dependencies": [
    {
      "package_name": "pandas",
      "version": ">=1.0.0",
      "optional": true,
      "description": "知识库图谱缓存所需（用于数据处理）"
    },
    {
      "package_name": "pyarrow",
      "version": ">=10.0.0",
      "optional": true,
      "description": "知识库图谱缓存所需（用于高效数据序列化）"
    },
    {
      "package_name": "quick_algo",
      "version": ">=0.1.0",
      "optional": true,
      "description": "知识库图谱缓存所需（用于图数据处理）",
      "install_name": "quick-algo"
    },
    {
      "package_name": "rapidfuzz",
      "version": ">=3.0.0",
      "optional": true,
      "description": "Levenshtein 距离加速模块（levenshtein_fast）可选依赖"
    },
    {
      "package_name": "pyahocorasick",
      "version": ">=2.0.0",
      "optional": true,
      "description": "黑话匹配自动机模块（jargon_matcher_automaton）可选依赖"
    }
  ],
  "id": "城陌.CM-performance-optimizer-plugin",
  "host_application": {
    "min_version": "0.12.0"
  },
  "repository_url": "https://github.com/chengmoya/CM-performance-optimizer-plugin",
  "keywords": [
    "performance",
    "cache",
    "optimization",
    "message-cache",
    "knowledge-graph"
  ],
  "tags": [
    "性能优化",
    "缓存",
    "消息缓存",
    "知识图谱",
    "表达式缓存"
  ]
}
